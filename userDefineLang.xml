<NotepadPlus>
    <UserLang name="Markdown" ext="mkdn mkd md">
        <Settings>
            <Global caseIgnored="yes" escapeChar="\" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="yes" words2="yes" words3="yes" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">`00`00</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">| +</Keywords>
            <Keywords name="Comment">1&lt;!-- 2--&gt; 0# 0## 0### 0#### 0#####</Keywords>
            <Keywords name="Words1">http:// (http:// https:// (https:// ftp:// (ftp:// git:// (git:// (/ [ ![</Keywords>
            <Keywords name="Words2">** __ * _</Keywords>
            <Keywords name="Words3">&gt; --- ---- ----- ------ ------- -------- --------- ---------- ----------- ------------ ------------- -------------</Keywords>
            <Keywords name="Words4">&lt;</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="37A8ED" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="EF00FF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="00FFFF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="666666" bgColor="FFFFFF" fontName="" fontStyle="2" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="0004FF" bgColor="FFFFFF" fontName="" fontStyle="3" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="8000FF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="34A53E" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="804000" bgColor="FFFFFF" fontName="" fontStyle="2" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="00FF40" bgColor="FFFFFF" fontName="" fontStyle="2" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
